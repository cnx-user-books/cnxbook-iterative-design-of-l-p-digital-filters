                          <m:mtext>for</m:mtext>
                          <m:mspace width="4.pt"/>
                          <m:mspace width="0.277778em"/>
                          <m:mi>m</m:mi>
                          <m:mo>=</m:mo>
                          <m:mn>2</m:mn>
                          <m:mo>,</m:mo>
