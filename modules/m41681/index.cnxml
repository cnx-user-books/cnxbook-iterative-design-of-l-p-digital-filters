                        <m:mi>h</m:mi>
                        <m:mrow>
                          <m:mi>L</m:mi>
                          <m:mo>-</m:mo>
                          <m:mi>N</m:mi>
                          <m:mo>-</m:mo>
                          <m:mn>2</m:mn>
                        </m:mrow>
                      </m:msub>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:msub>
