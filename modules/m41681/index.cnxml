                      </m:msup>
                    </m:mrow>
                  </m:mfrac>
                </m:mfenced>
                <m:mo>-</m:mo>
                <m:mi>D</m:mi>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:msub>
                    <m:mi>ω</m:mi>
                    <m:mi>k</m:mi>
                  </m:msub>
                  <m:mo>)</m:mo>
